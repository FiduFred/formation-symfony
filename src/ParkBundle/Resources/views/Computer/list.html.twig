{% extends 'ParkBundle::base.html.twig' %}

{% block title %}
    Computer list
{% endblock %}


<table>
    <thead><td>ID</td><td>Computer</td><td>IP</td><td>Status</td></thead>
    <tbody>
    {% for computer in computerlist  %}
        <tr>
            <td>{{ computer.id }}</td>
            <td>{{ computer.name }}</td>
            <td>{{ computer.ip }}</td>
            <td>{%  if computer.enabled == false %} Disable {% else %} Enable {% endif %}</td>
        </tr>
    {% else %}
        No Data
    {% endfor %}
    </tbody>
</table>